<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.canvas.Canvas?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.Graph"
            minHeight="100" prefWidth="600.0"
            stylesheets="@../css/main.css" styleClass="graph-window">
    <VBox prefWidth="600" alignment="TOP_CENTER">
        <Canvas fx:id="canvas"/>
        <Button alignment="BOTTOM_CENTER" text="close" onAction="#returnToMainMenu"/>
    </VBox>

</AnchorPane>
